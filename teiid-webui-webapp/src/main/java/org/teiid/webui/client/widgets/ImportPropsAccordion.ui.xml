<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.teiid.webui.client.widgets">

	<b:PanelGroup b:id="importAccordion">
		<b:Panel>
			<b:PanelHeader>
				<b:Heading size="H5">
				    <b:Icon type="GEARS" />
					<b:Anchor dataToggle="COLLAPSE" dataParent="#importAccordion" dataTarget="#collapseImportAccordion" ui:field="accordionAnchor"></b:Anchor>
				</b:Heading>
			</b:PanelHeader>
			<b:PanelCollapse b:id="collapseImportAccordion" ui:field="collapseAccordion">
				<b:PanelBody>
				  <t:TranslatorImportPropertyEditor ui:field="propertyEditor"></t:TranslatorImportPropertyEditor>
				</b:PanelBody>
			</b:PanelCollapse>
		</b:Panel>
	</b:PanelGroup>
	
</ui:UiBinder>
